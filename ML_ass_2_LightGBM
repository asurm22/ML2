lude=[np.number]).columns.tolist()\n\n        numeric_pipeline = Pipeline(steps=[\n            ('imputer', SimpleImputer(strategy='median')),\n            ('scaler', StandardScaler())\n        ])\n\n        categorical_pipeline = Pipeline(steps=[\n            ('imputer', SimpleImputer(strategy='most_frequent')),\n            ('target_encoder', ce.TargetEncoder())\n        ])\n\n        self.preprocessor_ = ColumnTransformer(transformers=[\n            ('num', numeric_pipeline, self.numerical_features_),\n            ('cat', categorical_pipeline, self.categorical_features_)\n        ])\n\n        self.preprocessor_.fit(X, y)\n\n        num_features_after = self.numerical_features_\n        cat_features_after = self.categorical_features_\n        self.feature_names_out_ = num_features_after + cat_features_after\n\n        return self\n\n    def transform(self, X):\n        X_transformed = self.preprocessor_.transform(X)\n\n        return pd.DataFrame(X_transformed, columns=self.feature_names_out_, index=X.index)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T14:58:59.680011Z","iopub.execute_input":"2025-04-27T14:58:59.680330Z","iopub.status.idle":"2025-04-27T14:58:59.703921Z","shell.execute_reply.started":"2025-04-27T14:58:59.680307Z","shell.execute_reply":"2025-04-27T14:58:59.703006Z"}},"outputs":[],"execution_count":6},{"cell_type":"markdown","source":"# Feature Selection","metadata":{}},{"cell_type":"code","source":"X = df_train.drop(columns=['isFraud', 'TransactionID', 'TransactionDT']) \ncl = Cleaner()\nX_cl = cl.fit_transform(X, y)\nfe = FeatureEngineer()\nX_fe = fe.fit_transform(X_cl, y)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T14:58:59.704812Z","iopub.execute_input":"2025-04-27T14:58:59.705124Z","iopub.status.idle":"2025-04-27T15:00:13.963364Z","shell.execute_reply.started":"2025-04-27T14:58:59.705098Z","shell.execute_reply":"2025-04-27T15:00:13.962115Z"}},"outputs":[],"execution_count":7},{"cell_type