p(columns=self.cols_to_drop_, errors='ignore')\n        return X_cleaned","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T16:43:09.842657Z","iopub.execute_input":"2025-04-27T16:43:09.842900Z","iopub.status.idle":"2025-04-27T16:43:09.848577Z","shell.execute_reply.started":"2025-04-27T16:43:09.842881Z","shell.execute_reply":"2025-04-27T16:43:09.847694Z"}},"outputs":[],"execution_count":5},{"cell_type":"markdown","source":"# Feature Engineering","metadata":{}},{"cell_type":"code","source":"class FeatureEngineer(BaseEstimator, TransformerMixin):\n    def __init__(self):\n        self.preprocessor_ = None\n        self.feature_names_out_ = None  \n\n    def fit(self, X, y=None):\n        self.categorical_features_ = X.select_dtypes(include=['object']).columns.tolist()\n        self.numerical_features_ = X.select_dtypes(include=[np.number]).columns.tolist()\n\n        numeric_pipeline = Pipeline(steps=[\n            ('imputer', SimpleImputer(strategy='median')),\n            ('scaler', StandardScaler())\n        ])\n\n        categorical_pipeline = Pipeline(steps=[\n            ('imputer', SimpleImputer(strategy='most_frequent')),\n            ('target_encoder', ce.TargetEncoder())\n        ])\n\n        self.preprocessor_ = ColumnTransformer(transformers=[\n            ('num', numeric_pipeline, self.numerical_features_),\n            ('cat', categorical_pipeline, self.categorical_features_)\n        ])\n\n        self.preprocessor_.fit(X, y)\n\n        # Save feature names\n        num_features_after = self.numerical_features_\n        cat_features_after = self.categorical_features_\n        self.feature_names_out_ = num_features_after + cat_features_after\n\n        return self\n\n    def transform(self, X):\n        X_transformed = self.preprocessor_.transform(X)\n        return pd.DataFrame(X_transformed, columns=self.feature_names_out_, index=X.index)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T16:43:09.849523Z",