         \n        for feat1, feat2, _ in self.high_corr_pairs:\n            if abs(X[feat1].corr(y)) < abs(X[feat2].corr(y)):\n                self.features_to_drop.append(feat1)\n            else:\n                self.features_to_drop.append(feat2)\n        \n        self.features_to_drop = list(set(self.features_to_drop))\n        return self\n\n    \n    def transform(self, X):\n        X = X.copy()\n        drop_feats = [f for f in self.features_to_drop if f in X.columns]\n        return X.drop(columns=drop_feats)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T16:21:33.919312Z","iopub.execute_input":"2025-04-27T16:21:33.920212Z","iopub.status.idle":"2025-04-27T16:21:33.930411Z","shell.execute_reply.started":"2025-04-27T16:21:33.920178Z","shell.execute_reply":"2025-04-27T16:21:33.929382Z"}},"outputs":[],"execution_count":15},{"cell_type":"code","source":"from sklearn.linear_model import LogisticRegression\npipeline = Pipeline(steps=[\n    ('cleaning', Cleaner(null_threshold=0.9)),\n    ('feature_engineering', FeatureEngineer()),\n    ('feature_select', CorrelationFeatureDropper()),\n    ('model', LogisticRegression(\n        solver='saga',   \n        max_iter=300,\n        penalty='l2',\n        C=1.0,\n        random_state=42,\n        n_jobs=-1\n    ))\n])\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T16:21:36.452841Z","iopub.execute_input":"2025-04-27T16:21:36.453403Z","iopub.status.idle":"2025-04-27T16:21:36.462600Z","shell.execute_reply.started":"2025-04-27T16:21:36.453369Z","shell.execute_reply":"2025-04-27T16:21:36.460420Z"}},"outputs":[],"execution_count":16},{"cell_type":"code","source":"X = df_train.drop(columns=['isFraud', 'TransactionID', 'TransactionDT'])\nX_train, X_val, y_train, y_val = train_test_split(\n    X, y, test_size=0.2, stratify=y, random_state=42\n)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-27T16:21:38.728374Z","iopub.execute_input":"2025-04-27T16:21:38